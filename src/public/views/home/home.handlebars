            <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeDouble ui-state-error ui-corner-all'>
                <div class='ui-widgetForm-inputLabel'>Some Double Value </div>
                <div class='ui-widgetForm-inputValue'>
                    <input class="input-validation-error ui-widget-content ui-corner-all" id="SomeDouble" name="SomeDouble" type="text" value="67,89dsf" />
                </div>
                <div class='ui-widgetForm-inputError '>
                    <div><span class="field-validation-error">The value &#39;67,89dsf&#39; is not valid for Some Double Value.</span></div>
                </div>
                <div class='ui-carriageReturn'></div>
            </div>

            <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeBoolean '>
                <div class='ui-widgetForm-inputLabel'>Some Boolean Value </div>
                <div class='ui-widgetForm-inputValue'>

                    <div data-widget="ui-widgetBoolean" data-widget-nullable="false">
                        <button class="ui-button-icon-only ui-button ui-state-default ui-corner-all" 
                                data-textOnly="true" 
                                data-widget="widgetButton" 
                                type="button">
                            <span class="ui-button-text">&nbsp;</span>
                            <span class="ui-icon-closethick ui-button-icon-primary ui-icon"></span>
                        </button>
                        <input id="SomeBoolean" name="SomeBoolean" type="checkbox" value="true" />
                        <input name="SomeBoolean" type="hidden" value="false" />
                    </div>

                </div>
                <div class='ui-widgetForm-inputError ui-hidden'></div>
                <div class='ui-carriageReturn'></div>
            </div>

            <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeBooleanNullable '>
                <div class='ui-widgetForm-inputLabel'>Some Boolean Nullable </div>
                <div class='ui-widgetForm-inputValue'>

                    <div data-widget="ui-widgetBoolean" class="ui-widgetBoolean" data-widget-nullable="true">
                        <button class="ui-button-icon-only ui-widgetButton ui-button ui-widget ui-state-default ui-corner-all" data-textonly="true" data-widget="widgetButton" type="button"><span class="ui-button-text">&nbsp;</span><span class="ui-icon-help ui-button-icon-primary ui-icon"></span></button>

                        <input id="SomeBooleanNullable" name="SomeBooleanNullable" type="checkbox" value="true" />
                        <input name="SomeBooleanNullable" type="hidden" value="" />
                    </div>

                </div>
                <div class='ui-widgetForm-inputError ui-hidden'></div>
                <div class='ui-carriageReturn'></div>
            </div>

            <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeStringArray '>
                <div class='ui-widgetForm-inputLabel'>Some String Array </div>
                <div class='ui-widgetForm-inputValue'>
                    <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeStringArray '>
                        <div class='ui-widgetForm-inputLabel'><label>Administrator</label></div>
                        <div class='ui-widgetForm-inputValue '>
                            <div data-widget='ui-widgetBoolean' class='ui-widgetBoolean'>
                                <input type='checkbox' id='SomeStringArray_Administrator' name='SomeStringArray' value='Administrator' />
                                <button class="ui-button-icon-only ui-widgetButton ui-button ui-widget ui-state-default ui-corner-all" data-checkboxid="SomeStringArray_Administrator" data-textonly="true" data-widget="widgetButton" type="button"><span class="ui-button-text">&nbsp;</span><span class="ui-icon-closethick ui-button-icon-primary ui-icon"></span></button>
                            </div>
                        </div>
                        <div class='ui-widgetForm-inputError ui-hidden'></div>
                        <div class='ui-carriageReturn'></div>
                    </div>
                    <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeStringArray '>
                        <div class='ui-widgetForm-inputLabel'><label>Guest</label></div>
                        <div class='ui-widgetForm-inputValue '>
                            <div data-widget='ui-widgetBoolean' class='ui-widgetBoolean'>
                                <input type='checkbox' id='SomeStringArray_Guest' name='SomeStringArray' value='Guest' />
                                <button class="ui-button-icon-only ui-widgetButton ui-button ui-widget ui-state-default ui-corner-all" data-checkboxid="SomeStringArray_Guest" data-textonly="true" data-widget="widgetButton" type="button"><span class="ui-button-text">&nbsp;</span><span class="ui-icon-closethick ui-button-icon-primary ui-icon"></span></button>
                            </div>
                        </div>
                        <div class='ui-widgetForm-inputError ui-hidden'></div>
                        <div class='ui-carriageReturn'></div>
                    </div>
                </div>
                <div class='ui-widgetForm-inputError ui-hidden'></div>
                <div class='ui-carriageReturn'></div>
            </div>

            <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeStringArrayII '>
                <div class='ui-widgetForm-inputLabel'>Another String Array </div>
                <div class='ui-widgetForm-inputValue'>
                    <select id="SomeStringArrayII" name="SomeStringArrayII">
                        <option selected="selected" value="Administrator">Administrator</option>
                        <option value="Guest">Guest</option>
                    </select>
                </div>
                <div class='ui-widgetForm-inputError ui-hidden'></div>
                <div class='ui-carriageReturn'></div>
            </div>

            <div data-widget='widgetModelItem' class='ui-widgetFormItem ui-widgetForm-SomeStringArrayII '>
                <div class='ui-widgetForm-inputLabel'>Another String Array </div>
                <div class='ui-widgetForm-inputValue'>
                    <div class='ui-widget-radioHorizontal'>
                        <div><label for="SomeStringArrayIII_Administrator" for="SomeStringArrayIII">Administrator</label></div>
                        <input checked="checked" id="SomeStringArrayIII_Administrator" name="SomeStringArrayIII" type="radio" value="Administrator" />
                    </div>
                    <div class='ui-widget-radioHorizontal'>
                        <div><label for="SomeStringArrayIII_Guest" for="SomeStringArrayIII">Guest</label></div>
                        <input id="SomeStringArrayIII_Guest" name="SomeStringArrayIII" type="radio" value="Guest" />
                    </div>
                </div>
                <div class='ui-widgetForm-inputError ui-hidden'></div>
                <div class='ui-carriageReturn'></div>
            </div>

            <div class="ui-widgetForm-buttons ui-widgetFormItem">
                <button class="ui-button-text-icon-primary ui-widgetButton ui-button ui-widget ui-state-default ui-corner-all" data-widget="widgetButton" type="submit"><span class="ui-button-text">Aceptar</span><span class="ui-icon-alert ui-button-icon-primary ui-icon"></span></button>
                <button class="ui-button-text-icon-primary ui-widgetButton ui-button ui-widget ui-state-default ui-corner-all" data-widget="widgetButton" type="button"><span class="ui-button-text">Validar</span><span class="ui-icon-alert ui-button-icon-primary ui-icon"></span></button>
            </div>

        </div>
    </div>

